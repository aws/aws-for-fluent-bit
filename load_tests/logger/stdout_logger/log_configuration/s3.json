{
	"logDriver":"awsfirelens",
	"options": {
		"Name": "s3",
		"region": "us-west-2",
		"bucket": "$S3_BUCKET_NAME",
		"total_file_size": "50M",
		"upload_timeout": "1m",
		"use_put_object": "On",
		"s3_key_format": "$STD_S3_OBJECT_NAME",
		"log-driver-buffer-limit": "536870911",
		"auto_retry_requests": "true",
		"workers": "1"
	}
}